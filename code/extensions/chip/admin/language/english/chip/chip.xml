<?xml version="1.0"?>
<definitions>
  <definition>
    <key>chip_name</key>
    <value><![CDATA[CHIP - Digital Finance Platform]]></value>
  </definition>
  <definition>
    <key>chip_api_secret</key>
    <value><![CDATA[Secret Key<span class="help">This can be retrieved from CHIP Dashboard >> Developer >> Keys.</span>]]></value>
  </definition>
  <definition>
    <key>chip_brand_id</key>
    <value><![CDATA[Brand ID<span class="help">This can be retrieved from CHIP Dashboard >> Developer >> Brands.</span>]]></value>
  </definition>
  <definition>
    <key>chip_payment_method_whitelist</key>
    <value><![CDATA[Payment Method Whitelist<span class="help">Setting whitelist will only allow specific payment method to be used for payment.</span>]]></value>
  </definition>
  <definition>
    <key>chip_payment_method_whitelist_fpx</key>
    <value><![CDATA[FPX (Online Banking)]]></value>
  </definition>
  <definition>
    <key>chip_payment_method_whitelist_fpx_b2b1</key>
    <value><![CDATA[FPX B2B1 (Corporate Online Banking)]]></value>
  </definition>
  <definition>
    <key>chip_payment_method_whitelist_mastercard</key>
    <value><![CDATA[Mastercard]]></value>
  </definition>
  <definition>
    <key>chip_payment_method_whitelist_maestro</key>
    <value><![CDATA[Maestro]]></value>
  </definition>
  <definition>
    <key>chip_payment_method_whitelist_visa</key>
    <value><![CDATA[Visa]]></value>
  </definition>
  <definition>
    <key>chip_payment_method_whitelist_razer_atome</key>
    <value><![CDATA[Razer Atome]]></value>
  </definition>
  <definition>
    <key>chip_payment_method_whitelist_razer_grabpay</key>
    <value><![CDATA[Razer GrabPay]]></value>
  </definition>
  <definition>
    <key>chip_payment_method_whitelist_razer_maybankqr</key>
    <value><![CDATA[Razer MaybankQR]]></value>
  </definition>
  <definition>
    <key>chip_payment_method_whitelist_razer_shopeepay</key>
    <value><![CDATA[Razer Shopeepay]]></value>
  </definition>
  <definition>
    <key>chip_payment_method_whitelist_razer_tng</key>
    <value><![CDATA[Razer TnG]]></value>
  </definition>
  <definition>
    <key>chip_payment_method_whitelist_duitnow_qr</key>
    <value><![CDATA[Duitnow QR]]></value>
  </definition>
  <definition>
    <key>chip_notify_customer_on_success</key>
    <value><![CDATA[Notify customer on success<span class="help">This to invoke notification once order updated to success state. AbanteCart will send email notification to customer.</span>]]></value>
  </definition>
  <definition>
    <key>chip_send_receipt</key>
    <value><![CDATA[Send Receipt<span class="help">This for CHIP to send receipt directly to customer. Turn off is recommended.</span>]]></value>
  </definition>
  <definition>
    <key>chip_due_strict</key>
    <value><![CDATA[Due Strict<span class="help">This to prevent payment after 1 hour. Turning this off will enable the purchase to be paid anytime.</span>]]></value>
  </definition>
  <definition>
    <key>chip_status_success_paid</key>
    <value><![CDATA[Success<span class="help">This for order status after payment successful. Recommended to set as Processing.</span>]]></value>
  </definition>
  <definition>
    <key>chip_location_id</key>
    <value><![CDATA[Location:<span class="help">This to enable CHIP only for specific location.</span>]]></value>
  </definition>
  <definition>
    <key>chip_location_id_0</key>
    <value><![CDATA[All Locations]]></value>
  </definition>
  <definition>
    <key>chip_success_redirect</key>
    <value><![CDATA[Success Redirect<span class="help">This only meant for troubleshooting purpose. It should be set to Enable on production environment.</span>]]></value>
  </definition>
  <definition>
    <key>chip_success_redirect_0</key>
    <value><![CDATA[Enable]]></value>
  </definition>
  <definition>
    <key>chip_success_redirect_1</key>
    <value><![CDATA[Disable]]></value>
  </definition>
  <definition>
    <key>chip_success_callback</key>
    <value><![CDATA[Success Callback<span class="help">This only meant for troubleshooting purpose. It should be set to Enable on production environment.</span>]]></value>
  </definition>
  <definition>
    <key>chip_success_callback_0</key>
    <value><![CDATA[Enable]]></value>
  </definition>
  <definition>
    <key>chip_success_callback_1</key>
    <value><![CDATA[Disable]]></value>
  </definition>
  <definition>
    <key>error_api_secret</key>
    <value><![CDATA[Secret Key Required!]]></value>
  </definition>
  <definition>
    <key>error_brand_id</key>
    <value><![CDATA[Brand ID Required!]]></value>
  </definition>
  <definition>
    <key>chip_timezone</key>
    <value><![CDATA[Timezone<span class="help">This settings to ensure purchase timestamp is according to your timezone. Set to Asia/Kuala Lumpur for Malaysia.</span>]]></value>
  </definition>
  <definition>
    <key>chip_email_fallback</key>
    <value><![CDATA[Email fallback<span class="help">This to set client email address in the event of email address field is blank.</span>]]></value>
  </definition>
  <definition>
    <key>chip_automatic_currency_conversion</key>
    <value><![CDATA[Automatic currency conversion<span class="help">This to allow CHIP to automatically convert total to MYR in the event of default currency is set to other than MYR. It requires MYR currency to be set and conversion value is depends on AbanteCart configuration. This feature is experimental.</span>]]></value>
  </definition>
</definitions>